{
  "name": "adesa-announcements",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "npm run build:app && concurrently -k -n build,serve \"npm:watch\" \"ng serve app-shell\"",
    "start:amp": "concurrently -k -n build,serve \"npm:watch\" \"http-server dist -p 52387 --cors -c-1\"",
    "build": "npm run build:app -- --prod",
    "build:app": "ng build adesa-announcements --output-hashing none --single-bundle=true --bundle-styles=false --keep-styles=false --extra-webpack-config webpack.extra.js",
    "build:app-shell": "ng build app-shell --prod",
    "build:elements": "node scripts/build-elements.js",
    "build:gzip": "node scripts/gzip-build.js \"dist/elements\"",
    "test": "ng test",
    "test:ci": "ng test adesa-announcements --codeCoverage=true --browsers=ChromeHeadless --watch=false",
    "lint": "ng lint",
    "lint:e2e": "tslint --format stylish --project e2e/tsconfig.json",
    "lint:prettier": "prettier --list-different \"**/*.{js,css,scss}\"",
    "lint:styles": "stylelint \"**/*.scss\" --syntax scss --custom-formatter=node_modules/stylelint-formatter-pretty",
    "watch": "npm run build:app -- --watch"
  },
  "private": true,
  "dependencies": {
    "@adesa/authorization": "^2.0.0",
    "@adesa/component-authorization": "^1.7.0",
    "@angular/animations": "^9.1.12",
    "@angular/cdk": "^9.2.4",
    "@angular/common": "^9.1.12",
    "@angular/compiler": "^9.1.12",
    "@angular/core": "^9.1.12",
    "@angular/elements": "^9.1.12",
    "@angular/flex-layout": "^9.0.0-beta.31",
    "@angular/forms": "^9.1.12",
    "@angular/material": "^9.2.4",
    "@angular/platform-browser": "^9.1.12",
    "@angular/platform-browser-dynamic": "^9.1.12",
    "@angular/router": "^9.1.12",
    "@material/chips": "^3.1.0",
    "@ngneat/transloco": "^2.17.4",
    "@ngneat/transloco-locale": "^1.3.1",
    "@ngneat/transloco-persist-translations": "^1.0.3",
    "core-js": "^2.6.11",
    "document-register-element": "^1.14.3",
    "material-design-icons": "^3.0.1",
    "ngx-mask": "^9.1.2",
    "rxjs": "~6.6.0",
    "tslib": "^1.13.0",
    "zone.js": "~0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/architect": "^0.901.11",
    "@angular-devkit/build-angular": "^0.901.11",
    "@angular-devkit/build-ng-packagr": "^0.901.11",
    "@angular-devkit/core": "^9.1.11",
    "@angular/cli": "^9.1.11",
    "@angular/compiler-cli": "^9.1.12",
    "@angular/language-service": "^9.1.12",
    "@types/jasmine": "3.3.3",
    "@types/jasminewd2": "^2.0.8",
    "@types/node": "^10.17.27",
    "@webcomponents/webcomponentsjs": "^2.4.3",
    "codelyzer": "^5.2.2",
    "concat": "^1.0.3",
    "concurrently": "^4.1.2",
    "custom-event-polyfill": "^1.0.7",
    "execa": "^1.0.0",
    "fs-extra": "^7.0.1",
    "http-server": "^0.12.3",
    "jasmine-core": "~3.4.0",
    "jasmine-marbles": "^0.5.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "^4.4.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "^2.1.1",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.5.4",
    "karma-junit-reporter": "^1.2.0",
    "ng-packagr": "^9.1.5",
    "ngx-build-plus": "^9.0.6",
    "prettier": "^1.19.1",
    "protractor": "^5.4.4",
    "puppeteer": "^1.9.0",
    "rxjs-tslint-rules": "^4.33.3",
    "stylelint": "^9.10.1",
    "stylelint-config-recommended": "^2.2.0",
    "stylelint-config-recommended-scss": "^3.3.0",
    "stylelint-formatter-pretty": "^1.1.4",
    "stylelint-scss": "^3.18.0",
    "ts-node": "^8.10.2",
    "tsickle": "^0.39.1",
    "tslint": "^5.20.1",
    "tslint-config-prettier": "^1.18.0",
    "tslint-consistent-codestyle": "^1.16.0",
    "tslint-plugin-prettier": "^2.3.0",
    "typescript": "~3.8.3",
    "webpack-concat-plugin": "^3.0.0"
  }
}
